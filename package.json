{
  "name": "ember-ajax",
  "version": "3.1.0",
  "description": "Service for making AJAX requests in Ember 1.13+ applications.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "url": "git@github.com:ember-cli/ember-ajax.git",
    "type": "git"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint --ext js .",
    "start": "ember server",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "ember-cli-babel": "^6.14.1",
    "najax": "^1.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@commitlint/travis-cli": "^7.0.0",
    "broccoli-asset-rev": "^2.7.0",
    "ember-cli": "~3.1.4",
    "ember-cli-chai": "^0.5.0",
    "ember-cli-dependency-checker": "^2.1.1",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "^0.15.0",
    "ember-cli-pretender": "^1.0.1",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-shims": "1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^2.1.0",
    "ember-cli-testdouble": "^0.1.3",
    "ember-cli-uglify": "^2.1.0",
    "ember-data": "^3.1.1",
    "ember-export-application-global": "2.0.0",
    "ember-load-initializers": "1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-mocha": "0.14.0",
    "ember-resolver": "4.5.6",
    "ember-source": "~3.2.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^0.2.23",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^1.0.0-rc.9",
    "lint-staged": "^7.2.0",
    "loader.js": "^4.7.0",
    "prettier": "^1.13.5",
    "testdouble": "^3.8.1",
    "testdouble-chai": "^0.5.0"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "fastbootDependencies": [
      "najax"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "broccoli-asset-rev",
      "ember-cli-babel",
      "ember-cli-dependency-checker",
      "ember-cli-htmlbars",
      "ember-cli-htmlbars-inline-precompile",
      "ember-cli-inject-live-reload",
      "ember-cli-shims",
      "ember-cli-sri",
      "ember-cli-test-loader",
      "ember-cli-uglify",
      "ember-data",
      "ember-export-application-global",
      "ember-load-initializers",
      "ember-resolver",
      "ember-source",
      "loader.js"
    ]
  }
}
